@import "../../search/search.wxss";

.l-title{
  height: 50rpx;
  width: 100%;
  background: #eee;
  font-size: 28rpx;
  padding-left:15rpx;
  line-height: 50rpx;
  letter-spacing: 2rpx;
}
.l-current{
  padding: 15rpx 30rpx 15rpx 15rpx;
  display: flex;
  font-size: 28rpx;
  justify-content: space-between;
  border: 1px solid #eee;
}

.l-position .l-c{
  margin-left: 20rpx;
}
.l-position :first-child{
  margin-left: 0;
}
.l-area{
  font-size: 30rpx;
  padding: 10rpx;
}
.l-sort{
  width: 100%;
  background: #eee;
  display: inline-block;
}
.l-city{
  line-height: 60rpx;
  border-bottom: 1px solid #eee;
}