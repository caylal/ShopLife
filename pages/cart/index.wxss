.cart{
  width: 100%;
  height: 100%;
  background-color: #eee;
}
.cart-title{
  height: 80rpx;
  width: 60%; 
  margin: 0 auto;
  text-align: center;  
}
.cart-title .title-text{
  display: table;
  width: 100%;
  color: #fc4104;
  font-size: 35rpx;
  padding-top: 20rpx;
  white-space: nowrap;
  border-spacing: 10px 0; 
}
.cart-title .title-text:before, .cart-title .title-text:after{
  display: table-cell;
  content: '';
  width:50%;
  background: -webkit-linear-gradient(#fc4104,#fc4104) repeat-x left center;
  background: linear-gradient(#fc4104,#fc4104) repeat-x left center;
  background-size: 1px 1px;
}
.cart-count{
  width: 100%;
  height: 80rpx;
  display: flex;
  position: fixed;
  justify-content: space-between;
  bottom: 0;
  background-color:#efeff4;
  border-top:1rpx solid #d7d6dc; 
}
.cart-count .cart-checked {
  display: flex;
  align-items: center;
  padding-left:15rpx;
}
.cart-count .cart-checked .allchecked{
  font-size: 30rpx;
}
.cart-count .cart-btn{
  text-align: center;  
  width: 120px;
  height: 100%;
  background-color: #07e307;
  line-height:80rpx;
  font-size:30rpx;
  color:#fff;
}
.cart-count .cart-text{
  font-size: 30rpx;
  display: flex;
  text-align: center;
  line-height: 80rpx;
}
.cart-count .cart-money{
  margin-right: 10rpx;
  color: #fc4104;
}
.cart-goods{
  width: 100%;
}
.cart-goods .cart-store{
  padding: 10rpx;
  border: 1px solid #efeff4;
  background-color: #fff;
}